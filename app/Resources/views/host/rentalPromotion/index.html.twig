{% extends 'host/base.html.twig' %}
{% block table %}
    <div class="row">
         <a href="{{ path('rentals_promotion_new') }}">
             <input type="button" class="btn btn-primary" value="Add">
         </a>
        {% for rentalPromotion in rentalsPromotions %}
            {{ rentalPromotion.startDate|date("m/d/Y")}}
            {{ rentalPromotion.endDate|date("m/d/Y")}}
            {{ rentalPromotion.reduction }}
            <br><br>
        {% endfor %}
    </div>
    <script>
        jQuery(document).ready(function() {
            FancyBox.initFancybox();
        });
    </script>
{% endblock %}
